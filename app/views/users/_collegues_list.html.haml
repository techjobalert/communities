- if users.present?
  - users.each do |user|
    .b-colleagues-item
      = image_tag user.avatar_url(:thumb_60), :class => 'popup-user-info', :id => "popup-user-#{user.id}"
      = link_to raw(user.full_name.gsub(" ","<br>")), user_path(user), :remote => :true, :class => 'title'

- else
  .no-items No items

.clear